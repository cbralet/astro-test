---
import HeadLayout from '@layouts/Head.astro'
import BodyLayout from '@layouts/Body.astro'
const { title = '' } = Astro.props
---
<!DOCTYPE html>
<html>
  <HeadLayout title={title}/>
  <BodyLayout>
    <slot name="body-top" slot="body-top"><!-- none in Body top --></slot>
      <slot name="main-top" slot="main-top"><!-- none in Body main top --></slot>
      <slot name="main-middle" slot="main-middle"><!-- none in Body main middle --></slot>
      <slot name="main-bottom" slot="main-bottom"><!-- none in Body main bottom --></slot>
    <slot name="body-bottom" slot="body-bottom"><!-- none in Body bottom --></slot>
  </BodyLayout>
</html>